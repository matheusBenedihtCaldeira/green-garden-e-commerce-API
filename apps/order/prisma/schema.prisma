generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Customer {
  id String @id @default(uuid())
  exeternalId String
  email String

  @@map("customers")
}

model Product {
  id String @id @default(uuid())
  name String
  exeternalId String
  bar_code String

  @@map("products")
}
