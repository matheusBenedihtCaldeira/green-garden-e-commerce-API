generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Customer {
  id String @id @default(uuid())
  name String
  lastName String
  email String 
  telefone String
  senha String
  createdAt DateTime @default(now())

  @@map("customers")
}