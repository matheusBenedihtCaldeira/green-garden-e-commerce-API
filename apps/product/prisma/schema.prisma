generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id String @id @default(uuid())
  name String
  description String
  price Float
  quantity Int
  bar_code String @unique
  createdAt DateTime @default(now())

  @@map("products")
}
